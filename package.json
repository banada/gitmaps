{
  "name": "gitmaps",
  "version": "0.1.0",
  "description": "Use git to edit graphs",
  "main": "index.js",
  "scripts": {
    "build:styles:static": "tailwindcss build src/client/static/static.css -o dist/static.min.css -c static.tailwind.config.js",
    "build:styles:app": "tailwindcss build src/client/components/styles/app.css -o dist/app.min.css -c tailwind.config.js",
    "build:styles": "npm run build:styles:static && npm run build:styles:app",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --no-devtool",
    "build": "npm run build:dev",
    "start:dev": "webpack --watch & npx nodemon --exec node --require dotenv/config lib/server.bundle.js",
    "start:prod": "NODE_ENV=production node --require dotenv/config lib/server.bundle.js",
    "deploy": "npm run build:styles && npm run build:prod && npm run start:prod",
    "start": "npm run start:dev"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.4.2",
    "axios": "^0.21.4",
    "babel-loader": "^8.0.5",
    "jest": "^26.6.3",
    "node-gyp": "^7.0.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-react": "^7.0.0",
    "@loadable/babel-plugin": "^5.7.2",
    "@loadable/component": "^5.7.0",
    "@loadable/server": "^5.7.0",
    "@loadable/webpack-plugin": "^5.7.1",
    "@sendgrid/mail": "^7.4.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.18.3",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "css-loader": "^3.5.3",
    "cytoscape-compound-drag-and-drop": "^1.0.0",
    "cytoscape-context-menus": "^4.1.0",
    "cytoscape-edgehandles": "^4.0.1",
    "cytoscape-popper": "^2.0.0",
    "dayjs": "^1.9.3",
